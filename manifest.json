{
  "manifest_version": 3,
  "name": "Writing Quotes",
  "version": "1.0",
  "description": "A Chrome Extension that displays great, randomized writing quotes on each new browser tab.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
